# platform :ios, '9.0'

target 'ChatApp' do
  use_frameworks!

# firebase pods
  pod 'Firebase/Analytics'
  pod 'Firebase/RemoteConfig'
  pod 'Firebase/Auth'
  pod 'FirebaseUI'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  
# FaceBook Sign in pods
  pod 'FBSDKLoginKit'

#Google Sign in pods 
pod 'GoogleSignIn'
  
end
